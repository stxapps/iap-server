  "dependencies": {
    "@google-cloud/datastore": "^6.6.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "googleapis": "^100.0.0",
    "node-apple-receipt-verify": "^1.12.1"
  }
}
